{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "description": "A simple pie chart with embedded data.",
    "data": {
        "url": "https://raw.githubusercontent.com/AnEvilBurrito/FIT3179/main/FinalData.csv"
    },
    "transform": [
        {
            "aggregate": [
                {
                    "op": "count",
                    "field": "Sector",
                    "as": "value"
                }
            ],
            "groupby": ["Sector"]
        }
    ],
    "mark": "arc",
    "encoding": {
        "theta": {
            "field": "value",
            "type": "quantitative"
        },
        "color": {
            "field": "Sector",
            "type": "nominal",
            "scale": {
                "scheme": "category20"
            },
            "legend": null
        }
    },
    "view": {
        "stroke": null
    }
}